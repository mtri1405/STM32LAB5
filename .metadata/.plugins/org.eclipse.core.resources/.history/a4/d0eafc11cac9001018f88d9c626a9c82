#include "fsm.h"


uint8_t buffer_character;
uint8_t buffer[MAX_BUFFER_SIZE];
bool cmdEqualRST(uint8_t str[]){
	if (strcmp((char*)str, "RST") == 0){
		return true;
	}
	else return false;
}

bool cmdEqualOK(uint8_t str[]){
	if (strcmp((char*) str, "OK") == 0) return true;
	else return false;
}
